<h1><%= @list.name %> </h1>
<br />

<% @items.each do |item| %>
	<h2><%= item.name %></h2>
	<h3><%= 'Completed' %>: <%= item.completed %></h3>
	<h3><%= 'Description' %>: <%= item.description %></h3>
	<% end %>
<%= link_to 'Edit', edit_list_path(@list) %>

<%= link_to 'Delete', list_path(@list), method: :delete %>

<%= link_to 'All Lists', lists_path %>
